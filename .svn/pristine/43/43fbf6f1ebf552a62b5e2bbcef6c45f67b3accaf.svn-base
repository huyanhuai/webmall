main {
    margin-top: 0.24rem;
}

.setting-item {
    width: 100%;
    height: 0.9rem;
    background: #fff;
    font-size: 0.3rem;
    line-height: 0.9rem;
}
.setting-item>label{
    width: 30%;
    float: left;
    height: 100%;
    color: #494949;
    padding-left: 0.5rem;
    border-bottom: 0.01rem solid #e5e5e5;
}
.setting-item>input,.setting-item>select{
    width: 70%;
    float: left;
    height: 100%;
    color: #a5a5a5;
    padding-left: 0.3rem;
    border-bottom: 0.01rem solid #e5e5e5;
    font-size: 0.26rem;
}
.setting-item>select{
    outline: none;
    border: none;
    appearance: none;
    -moz-appearance: none;
    -webkit-appearance: none;
    background: #fff;
    color: #6a6a6a;
}

/*头像*/
.head-image {
    width: 100%;
    height: 1.4rem;
    padding: 0 0.31rem 0 0.51rem;
    background: #fff;
    display: flex;
    align-items: center;
    justify-content: space-between;
    display: -webkit-flex;
    -webkit-align-items: center;
    -webkit-justify-content: space-between;
    font-size: 0.3rem;
    color: #333333;
    margin-bottom: 0.2rem;
}

.imgup {
    width: 1.12rem;
    height: 1.12rem;
    background: #fff;
    position: relative;
    border-radius: 50%;
}

.imgup img {
    width: 100%;
    height: 100%;
    border-radius: 50%;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
}

#imgup {
    height: 0;
    width: 0;
    z-index: -1;
    position: absolute;
    left: 10px;
    top: 5px;
}